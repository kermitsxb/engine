generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model Invoice {
  name           String
  amount         Int       @default(0)
  status         String    @default("draft")
  number         String?
  finalised_date DateTime?
  id             String    @id @default(cuid())
  created_at     DateTime  @default(now())
  updated_at     DateTime?
  deleted_at     DateTime?

  @@map("invoices")
}
