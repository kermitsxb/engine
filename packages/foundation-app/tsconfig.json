{
  "extends": "config-typescript/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/systems/types/**/*.ts",
    ".next/base_b51adfe7abaa9b8d9a044973e0e8b75d/types/**/*.ts",
    ".next/base_155d2badbbdd95f154c9422d0ce4932d/types/**/*.ts",
    ".next/base_087674eb974266bf4da5aa0b7e4481fe/types/**/*.ts",
    ".next/base_96750ead7dd92fb0044bc94f8a32e27e/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@database/client": [
        "../foundation-database/src/client"
      ],
      "@database/server": [
        "../foundation-database/src/server"
      ],
      "@database/*": [
        "../foundation-database/src/*"
      ],
      "@database": [
        "../foundation-database/src/shared"
      ],
      "@table/client": [
        "../foundation-table/src/client"
      ],
      "@table/server": [
        "../foundation-table/src/server"
      ],
      "@table/*": [
        "../foundation-table/src/*"
      ],
      "@table": [
        "../foundation-table/src/shared"
      ],
      "@common/server": [
        "../foundation-common/src/server"
      ],
      "@common/client": [
        "../foundation-common/src/client"
      ],
      "@common/*": [
        "../foundation-common/src/*"
      ],
      "@common": [
        "../foundation-common/src/shared"
      ],
      "@test": [
        "../utils-test/src/shared"
      ],
      "@test/server": [
        "../utils-test/src/server"
      ],
      "@test/client": [
        "../utils-test/src/client"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "ESNext",
      "target": "es6",
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@database/server": [
          "../foundation-database/src/server"
        ]
      }
    }
  }
}
