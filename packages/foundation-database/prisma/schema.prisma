generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model users {
  name       String    @default("")
  email      String    @default("")
  password   String    @default("")
  id         String    @id @default(uuid())
  created_at DateTime  @default(now())
  updated_at DateTime?
  deleted_at DateTime?
}
