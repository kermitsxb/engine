generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model users {
  id         Int      @id @default(autoincrement())
  name       String   @default("")
  email      String   @default("")
  password   String   @default("")
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}
