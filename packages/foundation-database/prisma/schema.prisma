generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model tableName {
  stringField                   String   @default("")
  stringWithDefaultValueField   String   @default("default value")
  integerField                  Int
  integerWithDefaultValueField  Int      @default(10)
  datetimeField                 DateTime @default(now())
  datetimeWithDefaultValueField DateTime @default(now())
  booleanField                  Boolean  @default(false)
  booleanWithDefaultValueField  Boolean  @default(true)
  id                            String   @id @default(uuid())
  created_at                    DateTime @default(now())
  updated_at                    DateTime @default(now())
  deleted_at                    DateTime @default(now())
}
