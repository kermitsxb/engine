{
  "database": {
    "url": "file:./test.db",
    "provider": "sqlite"
  },
  "tables": {
    "users": {
      "id": {
        "type": "integer",
        "primary": true,
        "generated": "increment"
      },
      "name": {
        "type": "string",
        "nullable": false
      },
      "email": {
