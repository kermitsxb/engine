generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./test.db"
}

model table {
  id                      Int      @id @default(autoincrement())
  string                  String
  integer                 Int
  integerWithDefaultValue Int      @default(0)
  datetime                DateTime @default(now())
}
