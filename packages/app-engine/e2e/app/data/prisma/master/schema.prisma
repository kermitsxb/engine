generator client {
  provider = "prisma-client-js"
  output   = "/Users/thomas/Documents/Codes/foundation/packages/app-engine/e2e/app/lib/prisma/master"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://prisma:prisma@localhost:5433/tests"
}

model Task {
  name        String
  description String?   @default("ceci est une description")
  number      Int       @unique
  priority    Int       @default(3)
  done_at     DateTime? @default(now())
  started_at  DateTime
  done        Boolean?
  open        Boolean?  @default(true)
  id          String    @id @default(cuid())
  created_at  DateTime  @default(now())
  updated_at  DateTime?
  deleted_at  DateTime?

  @@map("tasks")
}
