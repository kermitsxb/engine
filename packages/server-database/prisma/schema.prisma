generator client {
  provider      = "prisma-client-js"
  output        = "./client"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://admin:admin@localhost:5434/master"
}



























































































model Invoice {
      name String
id String @id @default(cuid())
created_at DateTime @default(now())
updated_at DateTime?
deleted_at DateTime?
  
      
      @@map("invoices")
    }
