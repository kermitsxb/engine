{
  "name": "@solumy/engine",
  "module": "dist/instrastructure/engine/app.js",
  "types": "dist/instrastructure/engine/app.d.ts",
  "type": "module",
  "scripts": {
    "format": "bunx prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "nodemon",
    "prebuild": "bunx puppeteer browsers install chrome",
    "build:server": "bun run scripts/server/build.ts",
    "build:client": "bun run scripts/client/build.ts",
    "build:styles": "bun run scripts/styles/build.ts",
    "build:types": "bun run scripts/types/build.ts",
    "build:schemas": "bun run scripts/schemas/build.ts",
    "build:utils": "bun run scripts/utils/build.ts",
    "build": "concurrently \"npm run build:schemas\" \"npm run build:client\" \"npm run build:server\" \"npm run build:types\" \"npm run build:styles\" \"npm run build:utils\"",
    "watch:server": "nodemon --config scripts/server/watch.json",
    "watch:client": "nodemon --config scripts/client/watch.json",
    "watch:styles": "nodemon --config scripts/styles/watch.json",
    "watch:types": "nodemon --config scripts/types/watch.json",
    "watch:schemas": "nodemon --config scripts/schemas/watch.json",
    "watch:utils": "nodemon --config scripts/utils/watch.json",
    "watch": "concurrently \"npm run watch:schemas\" \"npm run watch:client\" \"npm run watch:server\" \"npm run watch:types\" \"npm run watch:styles\" \"npm run watch:utils\"",
    "pretest": "bunx playwright install",
    "test": "bunx playwright test",
    "test:ui": "bunx playwright test --ui",
    "examples:request": "DEBUG=engine:* node examples/request/index.js",
    "examples:request:test": "DEBUG=engine:* node examples/request/tests.js",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@hotwired/turbo": "^7.3.0",
    "ajv": "^8.12.0",
    "better-sqlite3": "^9.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "kysely": "^0.27.2",
    "mailparser": "^3.6.7",
    "nanoid": "^5.0.4",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "puppeteer": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@types/better-sqlite3": "^7.6.9",
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.11.5",
    "@types/nodemon": "^1.19.6",
    "@types/pg": "^8.11.0",
    "@types/react-dom": "^18.2.18",
    "@types/smtp-server": "^3.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "concurrently": "^8.2.2",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "smtp-server": "^3.13.2",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.4.1",
    "typescript-json-schema": "^0.62.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": "1.0.26",
    "node": "20.11.0",
    "npm": "10.2.4"
  }
}
