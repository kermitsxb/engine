  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "build": "pnpm build:server & pnpm build:client",
    "build:server": "tsc -p src/tsconfig.server.json && node script/post-tsc.js",
    "build:client": "webpack && pnpm build:tailwind",
    "build:tailwind": "tailwindcss -i ./src/drivers/ui/TailwindUI/input.css -o ./dist/client/output.css --minify",
    "watch": "pnpm watch:server & pnpm watch:client & pnpm watch:tailwind",
    "watch:server": "tsc --watch",
    "watch:client": "NODE_ENV=development webpack --watch",
    "watch:tailwind": "tailwindcss -i ./src/drivers/ui/TailwindUI/input.css -o ./dist/client/output.css --watch",
    "watch:test": "jest --watch",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "jest",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui",
    "dev:ui": "storybook dev -p 6006",
    "dev": "NODE_ENV=development webpack serve --config webpack.config.ts & NODE_ENV=development nodemon dist/templates/invoices/startup.js",
    "start": "pnpm build && NODE_ENV=production node dist/templates/invoices/startup.js",
    "precommit": "pnpm format && pnpm lint && pnpm build && pnpm test",
    "prepare": "husky install",
    "ci": "pnpm install && pnpm exec playwright install --with-deps && pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_eae7002a5fff966"
  },
