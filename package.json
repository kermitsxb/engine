{
  "name": "@solumy/engine",
  "module": "dist/app.js",
  "types": "dist/app.d.ts",
  "type": "module",
  "scripts": {
    "format": "bunx prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "nodemon",
    "build:js": "bun run scripts/build/js/index.ts",
    "build:css": "bun run scripts/build/css/index.ts",
    "build:types": "bun run scripts/build/types/index.ts",
    "build:schemas": "bun run scripts/build/schemas/index.ts",
    "build": "concurrently \"npm run build:schemas\" \"npm run build:js\" \"npm run build:types\" \"npm run build:css\"",
    "watch:js": "nodemon --config scripts/build/js/nodemon.json",
    "watch:css": "nodemon --config scripts/build/css/nodemon.json",
    "watch:types": "nodemon --config scripts/build/types/nodemon.json",
    "watch:schemas": "nodemon --config scripts/build/schemas/nodemon.json",
    "watch": "concurrently \"npm run watch:schemas\" \"npm run watch:js\" \"npm run watch:types\" \"npm run watch:css\"",
    "pretest": "bunx playwright install && bun build ./tests/__helpers__/components.tsx --outdir ./tests/dist  --target node",
    "test": "bunx playwright test",
    "test:ui": "bunx playwright test --ui",
    "examples:request": "DEBUG=engine:* bun run --watch examples/request/index.ts",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "ajv": "^8.12.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "puppeteer": "^21.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.5",
    "@types/nodemon": "^1.19.6",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "concurrently": "^8.2.2",
    "debug": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.4.1",
    "typescript-json-schema": "^0.62.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": "1.0.25",
    "node": "21.6.1",
    "npm": "10.2.4"
  }
}
