{
  "name": "@solumy/engine",
  "main": "dist/bundle.js",
  "types": "dist/server.d.ts",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solumy/engine.git"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "build": "pnpm build:server && pnpm build:client && pnpm build:tailwind",
    "build:server": "webpack --config webpack.server.config.ts",
    "build:client": "webpack --config webpack.client.config.ts",
    "build:tailwind": "tailwindcss -i ./src/infrastructure/ui/TailwindUI/input.css -o ./dist/public/output.css --minify",
    "watch": "pnpm watch:typescript & pnpm watch:tailwind",
    "watch:typescript": "tsc --watch",
    "watch:test": "jest --watch",
    "watch:tailwind": "tailwindcss -i ./src/infrastructure/ui/TailwindUI/input.css -o ./dist/public/output.css --watch",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:utils": "jest --config test/utils/jest.config.ts",
    "test:unit": "jest --config test/unit/jest.config.ts",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui",
    "dev:invoices": "NODE_ENV=development webpack serve --config webpack.client.ts & NODE_ENV=development nodemon dist/templates/invoices/startup.js",
    "start:invoices": "pnpm build && NODE_ENV=production node dist/templates/invoices/startup.js",
    "precommit": "pnpm format && pnpm lint && pnpm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.6",
    "ajv": "8.12.0",
    "express": "^4.18.2",
    "fs-extra": "11.1.1",
    "handlebars": "^4.7.8",
    "luxon": "^3.4.2",
    "puppeteer": "^21.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.6.4",
    "@playwright/test": "1.33.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@swc/core": "^1.3.80",
    "@swc/jest": "^0.2.29",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "4.1.7",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "^29.5.4",
    "@types/luxon": "^3.3.1",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^18.17.12",
    "@types/pdf-parse": "^1.1.1",
    "@types/proper-lockfile": "^4.1.2",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.2",
    "@types/webpack": "^5.28.2",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.57.0",
    "babel-loader": "^9.1.3",
    "debug": "4.3.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "husky": "8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "29.5.0",
    "nanoid": "^3.3.6",
    "nodemon": "^3.0.1",
    "pdf-parse": "^1.1.1",
    "prettier": "latest",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  }
}
