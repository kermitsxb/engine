{
  "name": "@solumy/engine",
  "description": "Build internal tools with one config.",
  "version": "0.0.20",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "author": "Thomas JEANNEAU",
  "license": "Business Source License 1.1",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "build": "bun run build:server & bun run build:client & bun run build:types",
    "build:server": "bun run scripts/build-server.js",
    "build:client": "bun run scripts/build-client.js && bun run build:tailwind",
    "build:types": "tsc --project scripts/tsconfig.json && bun run scripts/build-types.js",
    "build:tailwind": "tailwindcss -i ./src/drivers/ui/TailwindUI/input.css -o ./dist/public/output.css --minify",
    "watch": "bun run watch:server & bun run watch:client & bun run watch:tailwind",
    "watch:server": "bun build ./src/server.ts --outdir ./dist --target bun --watch",
    "watch:client": "bun build ./src/client.tsx --outdir ./dist/public --watch",
    "watch:tailwind": "tailwindcss -i ./src/drivers/ui/TailwindUI/input.css -o ./dist/client/output.css --watch",
    "test": "bun run test:utils && bun run test:unit && bun run test:e2e",
    "test:unit": "cd src && bun test --bail --preload ../happydom.js",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui",
    "test:utils": "cd test && bun test",
    "example:invoices": "NODE_ENV=production bun run examples/invoices/startup.ts",
    "precommit": "bun run format && bun run lint && bun run build && bun run test",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_eae7002a5fff966"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solumy/engine.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/solumy/engine/issues"
  },
  "homepage": "https://github.com/solumy/engine#readme",
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.6",
    "express": "^4.18.2",
    "fp-ts": "^2.16.1",
    "fs-extra": "11.1.1",
    "handlebars": "^4.7.8",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "luxon": "^3.4.3",
    "puppeteer": "^21.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@faker-js/faker": "^8.1.0",
    "@happy-dom/global-registrator": "^12.2.0",
    "@playwright/test": "1.33.0",
    "@storybook/addon-essentials": "^7.4.3",
    "@storybook/addon-interactions": "^7.4.3",
    "@storybook/addon-links": "^7.4.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.3",
    "@storybook/react": "^7.4.3",
    "@storybook/react-webpack5": "^7.4.3",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/cors": "^2.8.14",
    "@types/debug": "4.1.7",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "^29.5.5",
    "@types/luxon": "^3.3.2",
    "@types/module-alias": "^2.0.2",
    "@types/node": "^18.17.18",
    "@types/pdf-parse": "^1.1.1",
    "@types/proper-lockfile": "^4.1.2",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.57.0",
    "bun-types": "^1.0.3",
    "chromatic": "^7.2.0",
    "debug": "4.3.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-storybook": "^0.6.14",
    "husky": "8.0.3",
    "pdf-parse": "^1.1.1",
    "prettier": "latest",
    "react-refresh": "^0.14.0",
